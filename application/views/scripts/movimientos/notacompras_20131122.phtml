            var rs=Ext.getCmp(notacompras.id+'-grid_lista').getSelectionModel().getSelected();
            if(rs)
            {				
				Ext.getCmp(notacompras.id+'-panel_derecho').expand();
				Ext.getCmp(notacompras.id+'-panel_derecho').setVisible(true);
				Ext.getCmp(notacompras.id+'-msganulado').setVisible(false);
                notacompras.fn_habilitar(true,2);
				if(rs.get('anulado')=='1'){
					Ext.getCmp(notacompras.id+'-btn_actualizar').setVisible(false);
					Ext.getCmp(notacompras.id+'-msganulado').setVisible(true);
				}
				
				Ext.getCmp(notacompras.id+'-nco_id').setValue(rs.get('nco_id'));
				
				var numero="000000000"+rs.get('codigo');
				var agencia= "000"+vgIdTienda;
				var codigo="NCC"+agencia.substr(agencia.length-3,3)+numero.substr(numero.length-9,9)
				
				Ext.getCmp(notacompras.id+'-codigo').setValue(codigo);
				Ext.getCmp(notacompras.id+'-tipo_ingreso').setValue(rs.get('tipo_ingreso'));
				Ext.getCmp(notacompras.id+'-fecha').setValue(rs.get('fec_ven'));
				
				Ext.getCmp(notacompras.id+'-prv_id').store.load({params:{campo:'prv_id',query:rs.get('prv_id'),start:0,limit:1}});
				Ext.getCmp(notacompras.id+'-prv_id').setValue(rs.get('prv_id'));
				Ext.getCmp(notacompras.id+'-prv_id').setRawValue(rs.get('nombre_proveedor'));
				
				Ext.getCmp(notacompras.id+'-prv_ruc').setValue(rs.get('ruc'));
				Ext.getCmp(notacompras.id+'-prv_codigo').setValue(rs.get('prv_codigo'));
				Ext.getCmp(notacompras.id+'-doc_id').setValue(rs.get('doc_id'));
				Ext.getCmp(notacompras.id+'-numero').setValue(rs.get('doc_n'));
				
				Ext.getCmp(notacompras.id+'-tnt_id').setValue(rs.get('tnt_id'));
				Ext.getCmp(notacompras.id+'-guia_remision').setValue(rs.get('n_guia'));
				Ext.getCmp(notacompras.id+'-cpa_id').setValue(rs.get('cpa_id'));
				Ext.getCmp(notacompras.id+'-mon_id').setValue(rs.get('mon_id'));
				Ext.getCmp(notacompras.id+'-observacion').setValue(rs.get('observacion'));
				
				if(rs.get('afecta')=='S'){
					Ext.getCmp(notacompras.id+'-afecta_almacen').setValue(true);
				}else{
					Ext.getCmp(notacompras.id+'-afecta_almacen').setValue(false);
				}
				if(rs.get('formato')=='S'){
					Ext.getCmp(notacompras.id+'-inc_igv').setValue(true);
				}else{
					Ext.getCmp(notacompras.id+'-inc_igv').setValue(false);	
				}
				Ext.Msg.wait('Cargando detalle de compra... por favor espere!');
				//alert(rs.get('nco_id'));
				Ext.Ajax.request({
					url:'movimientos/notacompras-detalle',
					params:
					{
						nco_id:rs.get('nco_id')
					},
					success:function(response,options)
					{     
						Ext.Msg.hide();        
						//alert(response.responseText);
						var res = Ext.decode(response.responseText);
						Ext.getCmp(notacompras.id+'-guias_asignadas').store.removeAll(false);						
						for (i=0;i<res.data.length;i++){
							//alert(res.data[i]);
							detalle = res.data[i];
							var u = new notacompras.store_reader.recordType({
								id:detalle['pro_id'],cantidad:detalle['cantidad'],codigo:detalle['codigo1'],detalle:detalle['producto'],precio:detalle['precio_compra'],descuento:detalle['descuento'],total:detalle['total'],ume_id:detalle['ume_id'], marca:detalle['marca']
							});
							//alert(detalle['pro_id']);
							Ext.getCmp(notacompras.id+'-guias_asignadas').stopEditing();
							notacompras.store_reader.insert(notacompras.store_reader.getCount(), u);
							Ext.getCmp(notacompras.id+'-guias_asignadas').startEditing(notacompras.store_reader.getCount()-1,1);
						}		
						//alert("salir");			
						notacompras.calcular();			
					}
				});
            }
            else
            {
                og.msg("Error","Seleccione un registro");
            }
