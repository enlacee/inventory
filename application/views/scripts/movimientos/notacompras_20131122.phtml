		fn_habilitar:function(sw, tipo)
        {
			if(!sw){
				Ext.getCmp(notacompras.id+'-btn_guardar').setVisible(sw);
				Ext.getCmp(notacompras.id+'-btn_actualizar').setVisible(sw);
			}else{
				if(tipo==1){
					Ext.getCmp(notacompras.id+'-btn_guardar').setVisible(sw);
					Ext.getCmp(notacompras.id+'-btn_actualizar').setVisible(!sw);
				}else{
					Ext.getCmp(notacompras.id+'-btn_guardar').setVisible(!sw);
					Ext.getCmp(notacompras.id+'-btn_actualizar').setVisible(sw);
				}
			}
			Ext.getCmp(notacompras.id+'-btn_nuevo').setVisible(!sw);			
			Ext.getCmp(notacompras.id+'-btn_cancelar').setVisible(sw);
			
			Ext.getCmp(notacompras.id+'-btn_editar').setVisible(!sw);
			Ext.getCmp(notacompras.id+'-btn_eliminar').setVisible(!sw);
