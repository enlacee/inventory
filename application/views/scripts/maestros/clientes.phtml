                callback:function()
                {
                    Ext.getCmp(clientes.id+'-ciu_id').setValue(rs.get('ciu_id'));
                    
                    
                }});
				Ext.getCmp(clientes.id+'-dis_id').store.load({params:{dep_id:rs.get('dep_id'),ciu_id:rs.get('ciu_id')},
