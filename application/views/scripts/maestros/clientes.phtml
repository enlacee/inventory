        fn_editar:function()
        {
            var rs=Ext.getCmp(clientes.id+'-grid_lista').getSelectionModel().getSelected();
            if(rs)
            {
				Ext.getCmp(clientes.id+'-panel_derecho').expand();
				Ext.getCmp(clientes.id+'-panel_derecho').setVisible(true);
                Ext.getCmp(clientes.id+'-cli_id').setValue(rs.get('cli_id'));
                Ext.getCmp(clientes.id+'-codigo').setValue(rs.get('codigo'));
                Ext.getCmp(clientes.id+'-nombre').setValue(rs.get('nombre'));
				
				Ext.getCmp(clientes.id+'-tip_per').setValue('N');
				if(rs.get('tip_per')!=null){
					Ext.getCmp(clientes.id+'-tip_per').setValue(rs.get('tip_per'));
					if(rs.get('tip_per')=='N'){
						 Ext.getCmp(clientes.id+'-tip_doc_d').setVisible(true);
						 Ext.getCmp(clientes.id+'-tip_doc_r').setVisible(true);
						 Ext.getCmp(clientes.id+'-tip_doc').setValue("D");
						 Ext.apply(Ext.getCmp(clientes.id+'-ruc'),{maxLength:8,minLength:8});
						 Ext.getCmp(clientes.id+'-ruc').el.dom.maxLength = 8;
					}else{
						 Ext.getCmp(clientes.id+'-tip_doc_d').setVisible(true);
						 Ext.getCmp(clientes.id+'-tip_doc_r').setVisible(true);
						 Ext.getCmp(clientes.id+'-tip_doc').setValue("D");
						 Ext.apply(Ext.getCmp(clientes.id+'-ruc'),{maxLength:11,minLength:11});
						 Ext.getCmp(clientes.id+'-ruc').el.dom.maxLength = 11;				
					}
				}				
				if(rs.get('tip_doc')!=null){
					Ext.getCmp(clientes.id+'-tip_doc').setValue(rs.get('tip_doc'));
				}
                Ext.getCmp(clientes.id+'-ruc').setValue(rs.get('ruc'));
                Ext.getCmp(clientes.id+'-telefono').setValue(rs.get('telefono'));
                Ext.getCmp(clientes.id+'-email').setValue(rs.get('email'));
                Ext.getCmp(clientes.id+'-web').setValue(rs.get('web'));
                Ext.getCmp(clientes.id+'-dep_id').setValue(rs.get('dep_id'));
				
