		fn_imprimir:function()
        {
			document.getElementById('frmReporte-clientes').action="maestros/clientes-lista-impresion";
			document.getElementById('txtpar1').value = Ext.getCmp(clientes.id+'-combo_describe').getValue();
			document.getElementById('txtpar2').value = Ext.getCmp(clientes.id+'-buscar').getValue();
			
			
			var activePage = Math.ceil((Ext.getCmp(clientes.id+'_pager').cursor + Ext.getCmp(clientes.id+'_pager').pageSize) / Ext.getCmp(clientes.id+'_pager').pageSize);
			document.getElementById('txtpar3').value = activePage;
			vOrden = Ext.getCmp(clientes.id+'-grid_lista').store.getSortState( );
			document.getElementById('txtpar4').value = Ext.getCmp(clientes.id+'-tip_per_buscar').getValue().getGroupValue();
			
			document.getElementById('txttitulo').value = "Listado de Clientes";
			vFiltro = '';
			if(Ext.getCmp(clientes.id+'-tip_per_buscar').getValue().getGroupValue()!=''){
				if(Ext.getCmp(clientes.id+'-tip_per_buscar').getValue().getGroupValue()=='N'){
					vFiltro = "Tipo de Persona : Natural";
				}else{
					vFiltro = "Tipo de Persona : Juridica";	
				}
			}
			if(Ext.getCmp(clientes.id+'-buscar').getValue()!=''){
				if(vFiltro!=''){
					vFiltro=vFiltro + ', ';
				}
				vFiltro = vFiltro + Ext.getCmp(clientes.id+'-combo_describe').getRawValue() + " : " + Ext.getCmp(clientes.id+'-buscar').getValue();
			}
			document.getElementById('txtfiltro').value = 'FILTRO : ' + vFiltro;
			document.getElementById('txtsort').value = vOrden.field;
			document.getElementById('txtdir').value = vOrden.direction;		
            document.getElementById('frmReporte-clientes').submit();
        },
