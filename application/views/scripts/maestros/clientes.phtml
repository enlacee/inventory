            if(!Ext.getCmp(clientes.id+'-nombre').isValid())
            {
                og.msg("Error", "Ingrese el campo Nombre");
                Ext.getCmp(clientes.id+'-nombre').focus(true,10);
                return false;
            }
			
            if(!Ext.getCmp(clientes.id+'-ruc').isValid())
            {
                if(Ext.getCmp(clientes.id+'-tip_doc').getValue().getGroupValue()=='D'){
	                og.msg("Error", "Ingrese el campo DNI");
				}else{
					og.msg("Error", "Ingrese el campo Ruc");
				}
                Ext.getCmp(clientes.id+'-ruc').focus(true,10);
                return false;
            }	
			
			/*if(!Ext.getCmp(clientes.id+'-email').isValid())
