                og.msg("Error", "E-mail no es valido");
                Ext.getCmp(clientes.id+'-email').focus(true,10);
                return false;
            }*/
			
            if(!Ext.getCmp(clientes.id+'-dep_id').isValid())
            {
                og.msg("Error", "Ingrese el campo Departamento");
                Ext.getCmp(clientes.id+'-dep_id').focus(true,10);
                return false;
            }
            if(!Ext.getCmp(clientes.id+'-ciu_id').isValid())
            {
                og.msg("Error", "Ingrese el campo Ciudad");
                Ext.getCmp(clientes.id+'-ciu_id').focus(true,10);
                return false;
            }
            if(!Ext.getCmp(clientes.id+'-dis_id').isValid())
            {
                og.msg("Error", "Ingrese el campo Distrito");
                Ext.getCmp(clientes.id+'-dis_id').focus(true,10);
